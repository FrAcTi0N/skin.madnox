<?xml version="1.0" encoding="utf-8"?>
<window>
	<defaultcontrol>20</defaultcontrol>
	<menucontrol>9050</menucontrol>
	<controls>
		<!-- Background -->
		<include content="BackgroundDefault">
			<param name="imagepath">$VAR[SystemBackgroundVar]</param>
		</include>
		<!-- Floor -->
		<control type="group">
			<visible>!Skin.HasSetting(SettingsHideFloor)</visible>
			<include content="AnimationSlideBottomWindow">
				<param name="slide">220</param>
				<param name="opencondition">$EXP[SystemAnimationOpenCondition]</param>
				<param name="closecondition">$EXP[SystemAnimationCloseCondition]</param>
			</include>
			<include content="AnimationSlideBottomVisible">
				<param name="slide">220</param>
			</include>
			<include>FloorDefault</include>
		</control>
		<!-- Panel -->
		<control type="group">
			<include>AnimationSlideLeftWindow</include>
			<include content="PanelDefault">
				<param name="right">965</param>
				<param name="righttexture">985</param>
				<param name="diffusetexture">diffuse/rounded20-835x880.png</param>
				<param name="reflect">!Skin.HasSetting(SettingsHideFloor)</param>
			</include>
		</control>
		<control type="group">
			<include>AnimationSlideRightWindow</include>
			<include content="PanelDefault">
				<param name="left">965</param>
				<param name="lefttexture">985</param>
				<param name="diffusetexture">diffuse/rounded20-835x880.png</param>
				<param name="reflect">!Skin.HasSetting(SettingsHideFloor)</param>
			</include>
		</control>
		<!-- Content -->
		<control type="group">
			<!-- Left Panel -->
			<control type="group">
				<include>AnimationSlideLeftWindow</include>
				<include content="FileManagerList">
					<param name="id">20</param>
					<param name="left">100</param>
					<param name="onleft">9050</param>
					<param name="onright">60</param>
					<param name="scrollbarid">60</param>
				</include>
				<control type="scrollbar" id="60">
					<top>144</top>
					<left>915</left>
					<bottom>144</bottom>
					<onleft>20</onleft>
					<onright>21</onright>
				</control>
				<control type="label" id="101">
					<bottom>102</bottom>
					<left>145</left>
					<width>615</width>
					<height>40</height>
					<font>Font22_Reg</font>
					<textcolor>$VAR[PanelSubTextColorVar]</textcolor>
					<haspath>true</haspath>
				</control>
			</control>
			<!-- Right Panel -->
			<control type="group">
				<include>AnimationSlideRightWindow</include>
				<include content="FileManagerList">
					<param name="id">21</param>
					<param name="left">985</param>
					<param name="onleft">60</param>
					<param name="onright">61</param>
					<param name="scrollbarid">61</param>
				</include>
				<control type="scrollbar" id="61">
					<top>144</top>
					<right>80</right>
					<bottom>144</bottom>
					<onleft>21</onleft>
				</control>
				<control type="label" id="102">
					<bottom>102</bottom>
					<left>1030</left>
					<width>615</width>
					<height>40</height>
					<font>Font22_Reg</font>
					<textcolor>$VAR[PanelSubTextColorVar]</textcolor>
					<haspath>true</haspath>
				</control>
			</control>
		</control>
		<!-- Topbar -->
		<control type="group">
			<visible>!Skin.HasSetting(SettingsHideTopBar)</visible>
			<include content="AnimationSlideTopWindow">
				<param name="slide">90</param>
				<param name="opencondition">$EXP[SystemAnimationOpenCondition]</param>
				<param name="closecondition">$EXP[SystemAnimationCloseCondition]</param>
			</include>
			<include content="AnimationSlideTopVisible">
				<param name="slide">90</param>
			</include>
			<include content="TopBar">
				<param name="label">$LOCALIZE[13000] / $LOCALIZE[7]</param>
				<param name="icon">files.png</param>
			</include>
		</control>
		<!-- Media Menu -->
		<include>MediaMenuMouseOverlay</include>
		<control type="group">
			<include>MediaMenuCommon</include>
			<control type="grouplist" id="9050">
				<onright>20</onright>
				<onback>20</onback>
				<include>MediaMenuSystem</include>
				<include>MediaMenuCommonBottom</include>
			</control>
		</control>
	</controls>
</window>