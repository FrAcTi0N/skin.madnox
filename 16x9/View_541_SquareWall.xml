					<include content="ViewArtworkAudioCase">
						<param name="diffuse"></param>
						<param name="bordersize">39,14,14,14</param>
					</include>
				</itemlayout>
				<focusedlayout height="343" width="368" condition="!Skin.HasSetting(View541SmallIcons) + Skin.HasSetting(View541AlbumCase) + [Container.Content(musicvideos) | Container.Content(albums)]">
