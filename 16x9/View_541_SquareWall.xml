<?xml version="1.0" encoding="utf-8"?>
<includes>
	<include name="View541SquareWall">
		<!-- Background fade -->
		<control type="group">
			<visible>Control.IsVisible(541)</visible>
			<include>AnimationVisibleFade</include>
			<include>AnimationWindowFade</include>
			<control type="image">
				<include>FullScreenDimensions</include>
				<texture colordiffuse="bbffffff">colors/black.png</texture>
			</control>
		</control>
		<!-- Panel -->
		<control type="group">
			<visible>Control.IsVisible(541)</visible>
			<animation effect="slide" end="0,-40" time="240" condition="Skin.HasSetting(ViewsHideTopBar)">Conditional</animation>
			<include>AnimationZoomWindow</include>
			<include>AnimationZoomVisible</include>
			<control type="panel" id="541">
				<top>80</top>
				<left>40</left>
				<width>1840</width>
				<height>1000</height>
				<onleft>9050</onleft>
				<onright>60515</onright>
				<pagecontrol>60515</pagecontrol>
				<viewtype label="$LOCALIZE[31255]">bigicon</viewtype>
				<itemlayout height="368" width="368" condition="!Skin.HasSetting(View541SmallIcons) + ![Container.Content(musicvideos) | Container.Content(albums)]">
					<include content="ViewArtwork">
						<param name="diffuse"></param>
					</include>
				</itemlayout>
				<focusedlayout height="368" width="368" condition="!Skin.HasSetting(View541SmallIcons) + ![Container.Content(musicvideos) | Container.Content(albums)]">
					<control type="group">
						<animation effect="zoom" start="100" end="115" center="auto" time="160">focus</animation>
						<animation effect="zoom" start="115" end="100" center="auto" time="160">unfocus</animation>
						<control type="image">
							<texture colordiffuse="$VAR[PanelHighlightColorVar]" border="10">buttons/thumb-select-focus.png</texture>
						</control>
						<include content="ViewArtwork">
							<param name="diffuse"></param>
						</include>
					</control>
				</focusedlayout>
				<itemlayout height="368" width="368" condition="!Skin.HasSetting(View541SmallIcons) + !Skin.HasSetting(View541AlbumCase) + [Container.Content(musicvideos) | Container.Content(albums)]">
					<include content="ViewArtwork">
						<param name="diffuse"></param>
					</include>
				</itemlayout>
				<focusedlayout height="368" width="368" condition="!Skin.HasSetting(View541SmallIcons) + !Skin.HasSetting(View541AlbumCase) + [Container.Content(musicvideos) | Container.Content(albums)]">
					<control type="group">
						<animation effect="zoom" start="100" end="115" center="auto" time="160">focus</animation>
						<animation effect="zoom" start="115" end="100" center="auto" time="160">unfocus</animation>
