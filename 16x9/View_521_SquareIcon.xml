<?xml version="1.0" encoding="utf-8"?>
<includes>
	<include name="View521SquareIcon">
		<!-- Floor -->
		<control type="group">
			<visible>Control.IsVisible(521)</visible>
			<include content="AnimationSlideBottomWindow">
				<param name="slide">220</param>
			</include>
			<control type="group">
				<visible>!Skin.HasSetting(View521HideFloor)</visible>
				<include content="Floor">
					<param name="highlight">FloorHighlightMiddle</param>
				</include>
			</control>
			<include content="FloorShadow">
				<param name="visible">Skin.HasSetting(View521HideFloor)</param>
			</include>
		</control>
		<!-- Panel -->
		<control type="group">
			<visible>Control.IsVisible(521)</visible>
			<include>AnimationZoomWindow</include>
			<include>AnimationZoomVisible</include>
			<include content="PanelDefault">
				<param name="top">142</param>
				<param name="height">822</param>
				<param name="toptexture">162</param>
				<param name="heighttexture">782</param>
				<param name="reflecttop">929</param>
				<param name="reflect">!Skin.HasSetting(View521HideReflect)</param>
			</include>
			<control type="panel" id="521">
				<top>212</top>
				<left>110</left>
				<width>1700</width>
				<height>682</height>
				<onleft>9050</onleft>
				<onright>60512</onright>
				<pagecontrol>60512</pagecontrol>
				<viewtype label="$LOCALIZE[31260]">bigicon</viewtype>
				<itemlayout height="341" width="340" condition="[!Skin.HasSetting(View521SmallIcons) + !Container.Content(albums)] | [!Skin.HasSetting(View521SmallIcons) + !Skin.HasSetting(View521AlbumCase) + Container.Content(albums)]">
					<include content="ViewArtwork">
						<param name="diffuse">diffuse/rounded5-320x320.png</param>
					</include>
				</itemlayout>
				<focusedlayout height="341" width="340" condition="[!Skin.HasSetting(View521SmallIcons) + !Container.Content(albums)] | [!Skin.HasSetting(View521SmallIcons) + !Skin.HasSetting(View521AlbumCase) + Container.Content(albums)]">
					<control type="image">
						<texture colordiffuse="$VAR[PanelHighlightColorVar]" border="10">buttons/thumb-select-focus.png</texture>
					</control>
					<control type="group">
						<animation  type="Focus" reversible="false">
							<effect type="zoom" start="95" end="100" time="300" center="auto" />
						</animation>
						<include content="ViewArtwork">
							<param name="diffuse">diffuse/rounded5-320x320.png</param>
						</include>
					</control>
				</focusedlayout>
				<itemlayout height="341" width="340" condition="!Skin.HasSetting(View521SmallIcons) + Skin.HasSetting(View521AlbumCase) + Container.Content(albums)">
					<include content="ViewArtworkAudioCase">
						<param name="bordersize">37,14,14,14</param>
					</include>
				</itemlayout>
				<focusedlayout height="341" width="340" condition="!Skin.HasSetting(View521SmallIcons) + Skin.HasSetting(View521AlbumCase) + Container.Content(albums)">
					<control type="image">
						<texture colordiffuse="$VAR[PanelHighlightColorVar]" border="10">buttons/thumb-select-focus.png</texture>
					</control>
					<control type="group">
						<animation  type="Focus" reversible="false">
							<effect type="zoom" start="95" end="100" time="300" center="auto" />
						</animation>
						<include content="ViewArtworkAudioCase">
							<param name="bordersize">37,14,14,14</param>
						</include>
					</control>
				</focusedlayout>
				<itemlayout height="227.32" width="242.85" condition="[Skin.HasSetting(View521SmallIcons) + !Container.Content(albums)] | [Skin.HasSetting(View521SmallIcons) + !Skin.HasSetting(View521AlbumCase) + Container.Content(albums)]">
					<control type="group">
						<include content="ViewArtwork">
							<param name="aspect">stretch</param>
							<param name="diffuse">diffuse/rounded5-223x207.png</param>
						</include>
					</control>
				</itemlayout>
				<focusedlayout height="227.32" width="242.85" condition="[Skin.HasSetting(View521SmallIcons) + !Container.Content(albums)] | [Skin.HasSetting(View521SmallIcons) + !Skin.HasSetting(View521AlbumCase) + Container.Content(albums)]">
					<control type="group">
						<top>3.2</top>
						<control type="image">
							<texture colordiffuse="$VAR[PanelHighlightColorVar]" border="10">buttons/thumb-select-focus.png</texture>
						</control>
						<control type="group">
							<animation  type="Focus" reversible="false">
								<effect type="zoom" start="95" end="100" time="300" center="auto" />
							</animation>
							<include content="ViewArtwork">
								<param name="aspect">stretch</param>
								<param name="diffuse">diffuse/rounded5-223x207.png</param>
							</include>
						</control>
					</control>
				</focusedlayout>
				<itemlayout height="227.32" width="242.85" condition="Skin.HasSetting(View521SmallIcons) + Skin.HasSetting(View521AlbumCase) + Container.Content(albums)">
					<include content="ViewArtworkAudioCase">
						<param name="bordersize">29,14,14,14</param>
					</include>
				</itemlayout>
				<focusedlayout height="227.32" width="242.85" condition="Skin.HasSetting(View521SmallIcons) + Skin.HasSetting(View521AlbumCase) + Container.Content(albums)">
					<control type="image">
						<texture colordiffuse="$VAR[PanelHighlightColorVar]" border="10">buttons/thumb-select-focus.png</texture>
					</control>
					<control type="group">
						<animation  type="Focus" reversible="false">
							<effect type="zoom" start="95" end="100" time="300" center="auto" />
						</animation>
						<include content="ViewArtworkAudioCase">
							<param name="bordersize">29,14,14,14</param>
						</include>
					</control>
				</focusedlayout>
			</control>
			<control type="scrollbar" id="60512">
				<right>80</right>
				<top>212</top>
				<bottom>186</bottom>
				<onleft>521</onleft>
			</control>
		</control>
		<!-- Bottom info/Media flags -->
		<control type="group">
			<visible>Control.IsVisible(521)</visible>
			<include content="AnimationSlideBottomWindow">
				<param name="slide">220</param>
			</include>
			<include content="ViewCoverFlowBottomInfo">
				<param name="logovisible">View521Logo</param>
			</include>
			<include condition="Window.IsVisible(MyMusicNav.xml)">MediaFlagsPanelAudio</include>
		</control>
	</include>
</includes>