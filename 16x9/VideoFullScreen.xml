<?xml version="1.0" encoding="utf-8"?>
<window>
	<defaultcontrol />
	<controls>
		<control type="grouplist">
			<include>AnimationVisibleFade</include>
			<itemgap>20</itemgap>
			<align>center</align>
			<orientation>horizontal</orientation>
			<include>FullscreenDimensions</include>
			<control type="label">
				<width>auto</width>
				<align>center</align>
				<font>Font120_Bold_Caps</font>
				<label>$VAR[SeekLabelVar]</label>
				<textcolor>$VAR[PanelSubTextColorVar]</textcolor>
			</control>
			<control type="label">
				<width>auto</width>
				<align>center</align>
				<font>Font120_Icon</font>
				<label>$VAR[SeekIconVar]</label>
				<textcolor>$VAR[PanelSubTextColorVar]</textcolor>
			</control>
		</control>
		<control type="group" id="0">
			<visible>Control.IsVisible(10)</visible>
			<width>1920</width>
			<height>162</height>
			<include content="AnimationSlideTopVisible">
				<param name="slide">162</param>
			</include>
			<include content="PanelWide">
				<param name="top">0</param>
				<param name="toptexture">0</param>
				<param name="topshadow">150</param>
				<param name="height">160</param>
				<param name="heighttexture">150</param>
				<param name="flipy">true</param>
			</include>
			<control type="grouplist">
				<top>16</top>
				<left>25</left>
				<width>1920</width>
				<height>600</height>
				<control type="label" id="10">
					<include>DialogLabelProcessInfo</include>
					<label></label>
				</control>
				<control type="label" id="11">
					<include>DialogLabelProcessInfo</include>
					<label></label>
				</control>
				<control type="label" id="12">
					<include>DialogLabelProcessInfo</include>
					<label></label>
				</control>
			</control>
		</control>
		<control type="label" id="1">
			<right>20</right>
			<top>33</top>
			<width>915</width>
			<height>60</height>
			<align>right</align>
			<label>$INFO[Player.SeekNumeric(hh:mm:ss)]</label>
			<font>Font_MainMenu</font>
			<shadowcolor>black</shadowcolor>
			<include>Animation_VisibleFade</include>
			<visible>!String.IsEmpty(Player.SeekNumeric)</visible>
		</control>
	</controls>
</window>