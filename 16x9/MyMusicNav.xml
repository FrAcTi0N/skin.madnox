		</include>
		<control type="fadelabel" id="398046">
			<top>-1000</top>
			<width>1</width>
			<height>1</height>
			<scroll>false</scroll>
			<randomize>true</randomize>
			<scrollout>false</scrollout>
			<pauseatend>5000</pauseatend>
			<resetonlabelchange>true</resetonlabelchange>
			<label>$INFO[Container(811111).ListItem(0).Art(fanart)]</label>
			<label>$INFO[Container(811111).ListItem(1).Art(fanart)]</label>
			<label>$INFO[Container(811111).ListItem(2).Art(fanart)]</label>
