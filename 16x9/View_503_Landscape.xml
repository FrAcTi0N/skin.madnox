						<visible>!Skin.HasSetting(View503HideReflect)</visible>
						<top>257</top>
						<height>272</height>
						<include content="ViewArtworkReflect">
							<param name="aspect">keep</param>
							<param name="art">$VAR[ClearLogoVar]</param>
						</include>
					</control>
					<include content="ViewArtwork">
						<param name="aspect">keep</param>
						<param name="art">$VAR[ClearLogoVar]</param>
