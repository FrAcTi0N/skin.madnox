<?xml version="1.0" encoding="utf-8"?>
<window id="1113" type="dialog">
	<defaultcontrol always="true">9090</defaultcontrol>
	<controls>
		<control type="group">
			<left>-240</left>
			<include content="AnimationDialogMediaMenuWindow">
				<param name="slide">240</param>
			</include>
			<include content="MediaMenuBackground">
				<param name="width">240</param>
				<param name="left">220</param>
			</include>
			<control type="panel" id="9090">
				<width>220</width>
				<height>1080</height>
				<onright>Dialog.Close(1113)</onright>
				<onback>Dialog.Close(1113)</onback>
				<itemlayout height="120" width="220">
					<control type="image">
						<texture border="30" colordiffuse="$VAR[MediaMenuListSeparatorColorVar]">buttons/list-select-nofocus.png</texture>
					</control>
					<control type="image">
						<top>14</top>
						<height>63</height>
						<aspectratio align="center" aligny="center">keep</aspectratio>
						<texture colordiffuse="$VAR[MediaMenuIconColorVar]">$INFO[ListItem.Property(MenuIcon)]</texture>
					</control>
					<control type="label">
						<top>66</top>
						<height>60</height>
						<align>center</align>
						<font>Font28_Reg</font>
						<textoffsetx>10</textoffsetx>
						<textcolor>$VAR[MediaMenuTextColorVar]</textcolor>
						<label>$INFO[ListItem.Label]</label>
					</control>
				</itemlayout>
				<focusedlayout height="120" width="220">
					<control type="image">
						<texture border="10" colordiffuse="$VAR[MediaMenuHighlightColorVar]">buttons/list-select-focus.png</texture>
					</control>
					<control type="image">
						<top>14</top>
						<height>63</height>
						<aspectratio align="center" aligny="center">keep</aspectratio>
						<texture colordiffuse="$VAR[MediaMenuFocusIconColorVar]">$INFO[ListItem.Property(MenuIcon)]</texture>
					</control>
					<control type="label">
						<top>66</top>
						<height>60</height>
						<align>center</align>
						<font>Font28_Reg</font>
						<textoffsetx>10</textoffsetx>
						<textcolor>$VAR[MediaMenuFocusTextColorVar]</textcolor>
						<label>$INFO[ListItem.Label]</label>
					</control>
				</focusedlayout>
				<content>
					<include>skinshortcuts-group-quicknav</include>
				</content>
			</control>
		</control>
	</controls>
</window>