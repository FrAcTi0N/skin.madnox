<?xml version="1.0" encoding="utf-8"?>
<includes>
	<default type="label">
		<label />
		<aligny>center</aligny>
		<font>Font30_Reg</font>
		<textcolor>$VAR[PanelSubTextColorVar]</textcolor>
		<disabledcolor>$VAR[PanelDisabledTextColorVar]</disabledcolor>
		<selectedcolor>$VAR[PanelHighlightColorVar]</selectedcolor>
		<scrollsuffix> • </scrollsuffix>
		<scrollspeed>8</scrollspeed>
		<scroll>false</scroll>
	</default>
	<default type="fadelabel">
		<aligny>center</aligny>
		<font>Font30_Reg</font>
		<textcolor>$VAR[PanelSubTextColorVar]</textcolor>
		<selectedcolor>$VAR[PanelHighlightColorVar]</selectedcolor>
		<scrollspeed>8</scrollspeed>
		<pauseatend>5000</pauseatend>
		<scrollout>false</scrollout>
	</default>
	<default type="edit">
		<textoffsetx>10</textoffsetx>
		<aligny>center</aligny>
		<texturefocus border="10" colordiffuse="$VAR[PanelHighlightColorVar]">buttons/list-select-focus.png</texturefocus>
		<texturenofocus border="30" colordiffuse="$VAR[PanelListSeparatorColorVar]">buttons/list-select-nofocus.png</texturenofocus>
		<font>Font30_Reg</font>
		<textcolor>$VAR[PanelSubTextColorVar]</textcolor>
		<focusedcolor>$VAR[PanelTextColorVar]</focusedcolor>
		<disabledcolor>$VAR[PanelDisabledTextColorVar]</disabledcolor>
		<invalidcolor>invalid</invalidcolor>
		<pulseonselect>false</pulseonselect>
	</default>
	<default type="textbox">
		<align>justify</align>
		<font>Font30_Reg</font>
		<textcolor>$VAR[PanelSubTextColorVar]</textcolor>
		<autoscroll time="3000" delay="4000" repeat="5000">true</autoscroll>
	</default>
	<default type="grouplist">
		<itemgap>0</itemgap>
	</default>
	<default type="image">
		<aspectratio>stretch</aspectratio>
		<texture />
	</default>
	<default type="multiimage">
		<fadetime>TextureFadeTime</fadetime>
		<timeperimage>7000</timeperimage>
	</default>
	<default type="button">
		<texturefocus border="10" colordiffuse="$VAR[PanelHighlightColorVar]">buttons/list-select-focus.png</texturefocus>
		<texturenofocus border="30" colordiffuse="$VAR[PanelListSeparatorColorVar]">buttons/list-select-nofocus.png</texturenofocus>
		<textoffsetx>10</textoffsetx>
		<font>Font30_Reg</font>
		<aligny>center</aligny>
		<scrollsuffix> • </scrollsuffix>
		<scrollspeed>10</scrollspeed>
		<textcolor>$VAR[PanelSubTextColorVar]</textcolor>
		<focusedcolor>$VAR[PanelFocusTextColorVar]</focusedcolor>
		<disabledcolor>$VAR[PanelDisabledTextColorVar]</disabledcolor>
		<invalidcolor>invalid</invalidcolor>
		<pulseonselect>false</pulseonselect>
	</default>
	<default type="radiobutton">
		<texturefocus border="10" colordiffuse="$VAR[PanelHighlightColorVar]">buttons/list-select-focus.png</texturefocus>
		<texturenofocus border="30" colordiffuse="$VAR[PanelListSeparatorColorVar]">buttons/list-select-nofocus.png</texturenofocus>
		<textureradioon colordiffuse="$VAR[PanelRadioButtonColorVar]">buttons/radiobutton-on.png</textureradioon>
		<textureradiooff colordiffuse="$VAR[PanelRadioButtonColorVar]">buttons/radiobutton-off.png</textureradiooff>
		<radiowidth>70</radiowidth>
		<radioheight>35</radioheight>
		<textoffsetx>10</textoffsetx>
		<font>Font30_Reg</font>
		<aligny>center</aligny>
		<scrollsuffix> • </scrollsuffix>
		<scrollspeed>10</scrollspeed>
		<textcolor>$VAR[PanelSubTextColorVar]</textcolor>
		<focusedcolor>$VAR[PanelFocusTextColorVar]</focusedcolor>
		<disabledcolor>$VAR[PanelDisabledTextColorVar]</disabledcolor>
		<pulseonselect>false</pulseonselect>
	</default>
	<default type="togglebutton">
		<texturefocus border="10" colordiffuse="$VAR[PanelHighlightColorVar]">buttons/list-select-focus.png</texturefocus>
		<texturenofocus border="30" colordiffuse="$VAR[PanelListSeparatorColorVar]">buttons/list-select-nofocus.png</texturenofocus>
		<alttexturefocus border="10" colordiffuse="$VAR[PanelHighlightColorVar]">buttons/list-select-focus.png</alttexturefocus>
		<alttexturenofocus border="30" colordiffuse="$VAR[PanelListSeparatorColorVar]">buttons/list-select-nofocus.png</alttexturenofocus>
		<textoffsetx>10</textoffsetx>
		<font>Font30_Reg</font>
		<aligny>center</aligny>
		<textcolor>$VAR[PanelSubTextColorVar]</textcolor>
		<focusedcolor>$VAR[PanelFocusTextColorVar]</focusedcolor>
		<disabledcolor>$VAR[PanelDisabledTextColorVar]</disabledcolor>
		<pulseonselect>false</pulseonselect>
	</default>
	<default type="spincontrol">
		<textureup colordiffuse="$VAR[PanelRadioButtonColorVar]">buttons/spin-up-nofocus.png</textureup>
		<texturedown colordiffuse="$VAR[PanelRadioButtonColorVar]">buttons/spin-down-nofocus.png</texturedown>
		<textureupfocus colordiffuse="$VAR[PanelRadioButtonColorVar]">buttons/spin-up-focus.png</textureupfocus>
		<texturedownfocus colordiffuse="$VAR[PanelRadioButtonColorVar]">buttons/spin-down-focus.png</texturedownfocus>
		<textureupdisabled colordiffuse="disabled">buttons/spin-up-nofocus.png</textureupdisabled>
		<texturedowndisabled colordiffuse="disabled">buttons/spin-down-nofocus.png</texturedowndisabled>
		<textoffsetx>10</textoffsetx>
		<font>Font30_Reg</font>
		<aligny>center</aligny>
		<textcolor>$VAR[PanelSubTextColorVar]</textcolor>
		<focusedcolor>$VAR[PanelFocusTextColorVar]</focusedcolor>
		<disabledcolor>$VAR[PanelDisabledTextColorVar]</disabledcolor>
		<pulseonselect>false</pulseonselect>
	</default>
	<default type="spincontrolex">
		<texturefocus border="10" colordiffuse="$VAR[PanelHighlightColorVar]">buttons/list-select-focus.png</texturefocus>
		<texturenofocus border="30" colordiffuse="$VAR[PanelListSeparatorColorVar]">buttons/list-select-nofocus.png</texturenofocus>
		<textureup colordiffuse="$VAR[PanelRadioButtonColorVar]">buttons/spin-up-nofocus.png</textureup>
		<texturedown colordiffuse="$VAR[PanelRadioButtonColorVar]">buttons/spin-down-nofocus.png</texturedown>
		<textureupfocus colordiffuse="$VAR[PanelRadioButtonColorVar]">buttons/spin-up-focus.png</textureupfocus>
		<texturedownfocus colordiffuse="$VAR[PanelRadioButtonColorVar]">buttons/spin-down-focus.png</texturedownfocus>
		<spinwidth>35</spinwidth>
		<spinheight>35</spinheight>
		<textoffsetx>10</textoffsetx>
		<font>Font30_Reg</font>
		<aligny>center</aligny>
		<textcolor>$VAR[PanelSubTextColorVar]</textcolor>
		<focusedcolor>$VAR[PanelFocusTextColorVar]</focusedcolor>
		<disabledcolor>$VAR[PanelDisabledTextColorVar]</disabledcolor>
		<pulseonselect>false</pulseonselect>
	</default>
	<default type="scrollbar">
		<width>40</width>
		<texturesliderbackground />
		<texturesliderbar />
		<texturesliderbarfocus />
		<textureslidernib>buttons/scrollbar-nib-nofocus.png</textureslidernib>
		<textureslidernibfocus colordiffuse="$VAR[PanelHighlightColorVar]">buttons/scrollbar-nib-focus.png</textureslidernibfocus>
		<showonepage>false</showonepage>
		<orientation>vertical</orientation>
	</default>
	<default type="progress">
		<height>20</height>
		<reveal>true</reveal>
		<texturebg border="8">panels/progress-control/progress-back.png</texturebg>
		<lefttexture />
		<midtexture colordiffuse="$VAR[PanelHighlightColorVar]" border="8">panels/progress-control/progress-mid.png</midtexture>
		<righttexture />
		<overlaytexture />
	</default>
	<default type="slider">
		<texturesliderbar border="15">buttons/slider-back.png</texturesliderbar>
		<textureslidernib>buttons/slider-nofocus.png</textureslidernib>
		<textureslidernibfocus>buttons/slider-focus.png</textureslidernibfocus>
		<orientation>horizontal</orientation>
		<width>200</width>
		<height>34</height>
	</default>
	<default type="sliderex">
		<texturesliderbar>buttons/slider-back.png</texturesliderbar>
		<textureslidernib colordiffuse="$VAR[PanelRadioButtonColorVar]">buttons/slider-nofocus.png</textureslidernib>
		<textureslidernibfocus colordiffuse="$VAR[PanelRadioButtonColorVar]">buttons/slider-focus.png</textureslidernibfocus>
		<texturefocus border="10" colordiffuse="$VAR[PanelHighlightColorVar]">buttons/list-select-focus.png</texturefocus>
		<texturenofocus border="30" colordiffuse="$VAR[PanelListSeparatorColorVar]">buttons/list-select-nofocus.png</texturenofocus>
		<sliderwidth>100</sliderwidth>
		<sliderheight>35</sliderheight>
		<aligny>center</aligny>
		<font>Font30_Reg</font>
		<textcolor>$VAR[PanelSubTextColorVar]</textcolor>
		<focusedcolor>$VAR[PanelFocusTextColorVar]</focusedcolor>
		<disabledcolor>$VAR[PanelDisabledTextColorVar]</disabledcolor>
		<pulseonselect>false</pulseonselect>
	</default>
</includes>