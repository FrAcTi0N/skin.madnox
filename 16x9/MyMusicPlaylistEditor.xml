<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol always="true">50</defaultcontrol>
	<views>50</views>
	<menucontrol>9050</menucontrol>
	<controls>
		<!-- Backdrop -->
		<include content="BackgroundDefault">
			<param name="imagepath">$VAR[MusicBackgroundVar]</param>
		</include>
		<!-- Floor -->
		<control type="group">
			<visible>!Skin.HasSetting(MusicHideFloor)</visible>
			<include content="AnimationSlideBottomWindow">
				<param name="slide">220</param>
			</include>
			<include content="AnimationSlideBottomVisible">
				<param name="slide">220</param>
			</include>
			<include>FloorDefault</include>
		</control>
		<!-- Panel -->
		<control type="group">
			<include>AnimationSlideLeftWindow</include>
			<include content="PanelDefault">
				<param name="right">965</param>
				<param name="righttexture">985</param>
				<param name="diffusetexture">diffuse/rounded20-835x880.png</param>
				<param name="reflect">!Skin.HasSetting(MusicHideFloor)</param>
			</include>
		</control>
		<control type="group">
			<include>AnimationSlideRightWindow</include>
			<include content="PanelDefault">
				<param name="left">965</param>
				<param name="lefttexture">985</param>
				<param name="diffusetexture">diffuse/rounded20-835x880.png</param>
				<param name="reflect">!Skin.HasSetting(MusicHideFloor)</param>
			</include>
		</control>
		<!-- Content -->
		<control type="group">
			<!-- Left Panel -->
			<control type="group">
				<include>AnimationSlideLeftWindow</include>
				<include content="FileManagerList">
					<param name="id">50</param>
					<param name="left">100</param>
					<param name="onleft">9050</param>
					<param name="onright">60</param>
					<param name="scrollbarid">60</param>
				</include>
				<control type="scrollbar" id="60">
					<top>144</top>
					<left>915</left>
					<bottom>144</bottom>
					<onleft>20</onleft>
					<onright>100</onright>
				</control>
			</control>
			<!-- Right Panel -->
			<control type="group">
				<include>AnimationSlideRightWindow</include>
				<include content="FileManagerList">
					<param name="id">100</param>
					<param name="left">985</param>
					<param name="onleft">100	</param>
					<param name="onright">61</param>
					<param name="scrollbarid">61</param>
				</include>
				<control type="scrollbar" id="61">
					<top>144</top>
					<right>80</right>
					<bottom>144</bottom>
					<onleft>100</onleft>
				</control>
			</control>
		</control>
		<!-- Topbar -->
		<control type="group">
			<visible>!Skin.HasSetting(MusicHideTopBar)</visible>
			<include content="AnimationSlideTopWindow">
				<param name="slide">90</param>
			</include>
			<include content="AnimationSlideTopVisible">
				<param name="slide">90</param>
			</include>
			<include content="TopBar">
				<param name="label">$LOCALIZE[10500]</param>
				<param name="icon">music.png</param>
			</include>
		</control>
		<!-- Media Menu -->
		<include>MediaMenuMouseOverlay</include>
		<control type="group">
			<include>MediaMenuCommon</include>
			<control type="grouplist" id="9050">
				<onright>50</onright>
				<onback>50</onback>
				<include>MediaMenuCommonTop</include>
				<!-- Open playlist -->
				<control type="radiobutton" id="6">
					<include content="MediaMenuIconButton">
						<param name="iconon">icons/mediamenu/open.png</param>
						<param name="iconoff">icons/mediamenu/open.png</param>
					</include>
					<label>$LOCALIZE[502]</label>
				</control>
				<!-- Save playlist -->
				<control type="radiobutton" id="7">
					<include content="MediaMenuIconButton">
						<param name="iconon">icons/mediamenu/save.png</param>
						<param name="iconoff">icons/mediamenu/save.png</param>
					</include>
					<label>$LOCALIZE[190]</label>
				</control>
				<!-- Clear playlist -->
				<control type="radiobutton" id="8">
					<include content="MediaMenuIconButton">
						<param name="iconon">icons/mediamenu/clear.png</param>
						<param name="iconoff">icons/mediamenu/clear.png</param>
					</include>
					<label>$LOCALIZE[192]</label>
				</control>
				<include>MediaMenuCommonBottom</include>
			</control>
		</control>
	</controls>
</window>