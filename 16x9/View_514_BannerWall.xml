<?xml version="1.0" encoding="utf-8"?>
<includes>
	<include name="View514BannerWall">
		<!-- Left Content -->
		<control type="group">
			<visible>Control.IsVisible(514)</visible>
			<include>AnimationZoomWindow</include>
			<include>AnimationZoomVisible</include>
			<include content="PanelDefault">
				<param name="top">132</param>
				<param name="height">832</param>
				<param name="toptexture">152</param>
				<param name="heighttexture">792</param>
				<param name="reflecttop">926</param>
				<param name="reflect">!Skin.HasSetting(ViewsHideFloor)</param>
			</include>
			<control type="panel" id="514">
				<top>202</top>
				<left>110</left>
				<width>1700</width>
				<height>692</height>
				<onleft>9050</onleft>
				<onright>60514</onright>
				<pagecontrol>60514</pagecontrol>
				<itemlayout height="173" width="850">
					<control type="image">
						<bordersize>10</bordersize>
						<aspectratio align="center" aligny="center">scale</aspectratio>
						<texture diffuse="diffuse/rounded5-532x800.png">$VAR[BannerVar]</texture>
					</control>
					<control type="image">
						<texture border="20">thumbs/overlay.png</texture>
					</control>
				</itemlayout>
				<focusedlayout height="173" width="850">
					<control type="image">
						<texture colordiffuse="$VAR[PanelHighlightColorVar]" border="10">buttons/widget-select-focus.png</texture>
					</control>
					<control type="group">
						<animation  type="Focus" reversible="false">
							<effect type="zoom" start="95" end="100" time="300" center="auto" />
						</animation>
						<control type="image">
							<bordersize>10</bordersize>
							<aspectratio align="center" aligny="center">scale</aspectratio>
							<texture diffuse="diffuse/rounded5-532x800.png">$VAR[BannerVar]</texture>
						</control>
						<control type="image">
							<texture border="20">thumbs/overlay.png</texture>
						</control>
					</control>
				</focusedlayout>
			</control>
			<control type="scrollbar" id="60514">
				<right>80</right>
				<top>192</top>
				<bottom>176</bottom>
				<onleft>514</onleft>
			</control>
		</control>
	</include>
</includes>