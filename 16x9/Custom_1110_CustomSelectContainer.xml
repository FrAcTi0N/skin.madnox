<?xml version="1.0" encoding="utf-8"?>
<window id="1110" type="dialog">
	<defaultcontrol always="true">100</defaultcontrol>
	<include>AnimationDialogPopupWindow</include>
	<onunload>ClearProperty(CustomSelect,home)</onunload>
	<controls>
		<control type="group">
			<visible>!Window.IsActive(DialogSelect.xml)</visible>
			<include>AnimationDialogPopupVisible</include>
			<include content="DialogBackground">
				<param name="width" value="1030" />
				<param name="height" value="580" />
				<param name="label">$INFO[Window(home).Property(CustomSelect)]</param>
				<param name="diffusetexture">diffuse/rounded10-990x540.png</param>
			</include>
			<control type="button" id="8000">
				<onleft>100</onleft>
				<ondown>100</ondown>
				<include>DialogButtonClose</include>
			</control>
			<control type="list" id="100">
				<top>90</top>
				<left>20</left>
				<width>990</width>
				<height>420</height>
				<onleft>60</onleft>
				<onright>8000</onright>
				<pagecontrol>60</pagecontrol>
				<include content="DialogList2Default">
					<param name="id">100</param>
				</include>
				<content>
					<!-- Backgrounds -->
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),homeicons)">SelectHomeIcons</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[3] $LOCALIZE[31107])">SelectVideoBackground</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[14204] $LOCALIZE[31107])">SelectPVRBackground</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[2] $LOCALIZE[31107])">SelectMusicBackground</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[15016] $LOCALIZE[31107])">SelectGamesBackground</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[1] $LOCALIZE[31107])">SelectPicturesBackground</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[24001] $LOCALIZE[31107])">SelectAddonBackground</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[8] $LOCALIZE[31107])">SelectWeatherBackground</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[13000] $LOCALIZE[31107])">SelectSystemBackground</include>

					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[31030])">SelectNowPlayingMusicBackground</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[31029])">SelectNowPlayingVideoBackground</include>
					<!-- Main menu -->
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[31041] $LOCALIZE[31144])">SelectMainMenuLayout</include>

					
					
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[31142])">SelectMainMenuSubmenuBar</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[31143])">SelectMainMenuRSSBar</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[31041] $LOCALIZE[31207])">SelectMainMenuPosition</include>
					<!-- Now playing -->
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[2] $LOCALIZE[31176])">SelectMainMenuNowPlayingMusicCase</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),CaseVideo)">SelectMainMenuNowPlayingVideoCase</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[31169])">SelectMainMenuNowPlayingMusicInfo</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[31170])">SelectMainMenuNowPlayingAudioInfo</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[31211])">SelectMainMenuNowPlayingMovieInfo</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[12003])">SelectMainMenuNowPlayingVideoInfo</include>
					<!-- Music viz -->
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[250] $LOCALIZE[31176])">SelectNowPlayingMusicVizCase</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[31177])">SelectNowPlayingMusicVizMusicInfo</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[31178])">SelectNowPlayingMusicVizAudioInfo</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[250] $LOCALIZE[31179])">SelectNowPlayingMusicVizFanArt</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[36427])">SelectNowPlayingMusicVizInfo</include>
					<!-- Other -->
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[31009])">SelectAudioCDArtFallback</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[31299])">SelectVideoCDArtFallback</include>
					<!-- Views -->
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[31230] $LOCALIZE[29915])">SelectShowcaseInfo</include>
					<!-- Favourites -->
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[31292])">SelectCustomFavourites</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[31294])">SelectCustomFavouriteAddons</include>
					<!-- Theme -->
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[15111])">SelectCustomTheme</include>
				</content>
			</control>
			<control type="image">
				<include>DialogHorizontalSeparator</include>
			</control>
			<control type="label">
				<include>DialogItemCountLabel</include>
				<label>$INFO[Container(100).NumItems,, $LOCALIZE[31001]]$INFO[Container(100).CurrentPage, - ]$INFO[Container(100).NumPages,/]</label>
			</control>
			<control type="scrollbar" id="60">
				<onright>100</onright>
				<include>DialogScrollBar</include>
			</control>
		</control>
	</controls>
</window>