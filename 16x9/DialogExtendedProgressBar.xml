<?xml version="1.0" encoding="utf-8"?>
<window>
	<controls>
		<control type="group">
			<animation effect="zoom" end="80" center="auto" time="240" tween="cubic" easing="inout" condition="Skin.HasSetting(SmallerNotifications)">Conditional</animation>
			<top>49</top>
			<centerleft>50%</centerleft>
			<height>134</height>
			<width>730</width>
			<control type="group">
				<include>AnimationDialogPopupWindow</include>
				<include content="AnimationDialogPopupHidden">
					<param name="condition">Window.IsActive(notification) | Window.IsActive(startup) | Window.IsActive(fullscreenvideo) | Window.IsActive(fullscreengame) | Window.IsActive(visualisation) | Window.IsActive(DialogMusicInfo.xml) | Window.IsActive(DialogVideoInfo.xml)</param>
				</include>
				<control type="image">
					<texture colordiffuse="$VAR[DialogShadowColorVar]" border="20">$VAR[DialogShadowScanVar]</texture>
				</control>
				<control type="image">
					<texture colordiffuse="$VAR[DialogStyleColorVar]" border="20">$VAR[DialogStyleScanVar]</texture>
				</control>
				<control type="image">
					<top>10</top>
					<left>10</left>
					<right>10</right>
					<bottom>10</bottom>
					<aspectratio scalediffuse="false">center</aspectratio>
					<texture diffuse="diffuse/rounded10-710x104.png" colordiffuse="$VAR[DialogTextureColorVar]">$INFO[Skin.String(DialogTexture.texture)]</texture>
					<visible>!Skin.HasSetting(DialogSquare.texture)</visible>
				</control>
				<control type="image">
					<top>10</top>
					<left>10</left>
					<right>10</right>
					<bottom>10</bottom>
					<aspectratio scalediffuse="false">center</aspectratio>
					<texture colordiffuse="$VAR[DialogTextureColorVar]">$INFO[Skin.String(DialogTexture.texture)]</texture>
					<visible>Skin.HasSetting(DialogSquare.texture)</visible>
				</control>
				<control type="image">
					<texture colordiffuse="$VAR[DialogEmbossColorVar]" border="40">$VAR[DialogEmbossScanVar]</texture>
				</control>
				<control type="image">
					<top>22</top>
					<left>26</left>
					<width>90</width>
					<height>90</height>
					<animation effect="rotate" end="-360" center="auto" time="2000" loop="true" condition="true">Conditional</animation>
					<aspectratio>keep</aspectratio>
					<texture colordiffuse="$VAR[DialogHighlightColorVar]">panels/spinner.png</texture>
				</control>
				<control type="label">
						<top>22</top>
					<left>26</left>
					<width>90</width>
					<height>90</height>
					<align>center</align>
						<font>Font29_Cond</font>
						<textcolor>$VAR[DialogTextColorVar]</textcolor>
						<label>$INFO[Control.GetLabel(32),,%]</label>
					</control>
				<control type="group">
					<top>15</top>
					<left>144</left>
					<control type="label" id="30">
						<description>Line 1 Label</description>
						<width>564</width>
						<height>40</height>
						<font>Font29_Cond</font>
						<textcolor>$VAR[DialogTextColorVar]</textcolor>
					</control>
					<control type="label">
						<description>Line 2 Label</description>
						<top>32</top>
						<width>564</width>
						<height>40</height>
						<font>Font29_Cond</font>
						<textcolor>$VAR[DialogSubTextColorVar]</textcolor>
						<label>$INFO[Control.GetLabel(31)]</label>
					</control>
					<control type="progress" id="32">
						<top>74</top>
						<width>564</width>
						<midtexture colordiffuse="$VAR[DialogHighlightColorVar]" border="8">panels/progress-control/progress-mid.png</midtexture>
					</control>
				</control>
			</control>
		</control>
		<control type="label" id="31">
			<description>Line 1 Label</description>
			<include>HiddenObject</include>
		</control>
	</controls>
</window>