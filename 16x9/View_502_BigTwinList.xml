<?xml version="1.0" encoding="utf-8"?>
<includes>
	<include name="View502BigTwinList">
		<!-- Left Content -->
		<control type="group">
			<visible>Control.IsVisible(502)</visible>
			<include>AnimationSlideLeftWindow</include>
			<include>AnimationSlideLeftVisible</include>
			<include content="PanelDefault">
				<param name="right">965</param>
				<param name="righttexture">985</param>
				<param name="diffusetexture">diffuse/rounded20-835x880.png</param>
				<param name="reflect">!Skin.HasSetting(ViewsHideFloor)</param>
			</include>
			<control type="panel" id="502">
				<left>100</left>
				<top>141</top>
				<width>835</width>
				<height>798</height>
				<onleft>9050</onleft>
				<onright>60502</onright>
				<pagecontrol>60502</pagecontrol>
				<viewtype label="$LOCALIZE[31218]">list</viewtype>
				<include>ViewContainerBigList</include>
			</control>
			<control type="scrollbar" id="60502">
				<top>140</top>
				<left>915</left>
				<bottom>140</bottom>
				<onleft>502</onleft>
				<onright>5200</onright>
			</control>
		</control>
		<!-- Right Content -->
		<control type="group">
			<visible>Control.IsVisible(502)</visible>
			<include>AnimationSlideRightWindow</include>
			<include>AnimationSlideRightVisible</include>
			<include content="PanelDefault">
				<param name="left">965</param>
				<param name="lefttexture">985</param>
				<param name="diffusetexture">diffuse/rounded20-835x880.png</param>
				<param name="reflect">!Skin.HasSetting(ViewsHideFloor)</param>
			</include>
			<control type="panel" id="5200">
				<left>985</left>
				<top>141</top>
				<width>835</width>
				<height>798</height>
				<onleft>6052</onleft>
				<onright>6050200</onright>
				<pagecontrol>6050200</pagecontrol>
				<itemlayout height="72.5">
					<control type="image">
						<texture border="30" colordiffuse="$VAR[PanelListSeparatorColorVar]">buttons/list-select-nofocus.png</texture>
					</control>
					<control type="label">
						<left>25</left>
						<right>20</right>
						<label>$INFO[ListItem.Label]</label>
						<textcolor>$VAR[PanelSubTextColorVar]</textcolor>
					</control>
					<control type="label">
						<right>25</right>
						<align>right</align>
						<label>$VAR[TwinListLabel2Var]</label>
						<textcolor>$VAR[PanelSubTextColorVar]</textcolor>
					</control>
				</itemlayout>
				<focusedlayout height="72.5">
					<control type="image">
						<texture border="30" colordiffuse="$VAR[PanelHighlightColorVar]">buttons/list-select-focus.png</texture>
					</control>
					<control type="label">
						<left>25</left>
						<right>20</right>
						<scroll>true</scroll>
						<label>$INFO[ListItem.Label]</label>
						<textcolor>$VAR[PanelFocusTextColorVar]</textcolor>
					</control>
					<control type="label">
						<right>25</right>
						<align>right</align>
						<label>$VAR[TwinListLabel2Var]</label>
						<textcolor>$VAR[PanelFocusTextColorVar]</textcolor>
					</control>
				</focusedlayout>
				<content target="$VAR[TwinListTargetVar]" sortby="" sortorder="">$INFO[Container(502).ListItem.FolderPath]</content>
			</control>
			<control type="scrollbar" id="6050200">
				<top>140</top>
				<right>80</right>
				<bottom>140</bottom>
				<onleft>5200</onleft>
			</control>
		</control>
	</include>
</includes>