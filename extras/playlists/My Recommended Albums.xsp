<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="albums">
    <name>My Recommended Albums</name>
    <match>all</match>
    <rule field="userrating" operator="greaterthan">
        <value>7</value>
    </rule>
    <limit>25</limit>
    <order direction="descending">userrating</order>
</smartplaylist>
