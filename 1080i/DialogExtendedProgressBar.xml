<?xml version="1.0" encoding="utf-8"?>
<window>
	<controls>
		<control type="group">
			<animation effect="zoom" end="80" center="auto" time="240" tween="cubic" easing="inout" condition="Skin.HasSetting(SmallerNotifications)">Conditional</animation>
			<top>49</top>
			<centerleft>50%</centerleft>
			<height>134</height>
			<width>730</width>
			<control type="group">
				<include>Animation_Dialog_Popup_Window</include>
				<include content="Animation_Dialog_Popup_Hidden">
					<param name="condition">Window.IsActive(notification) | Window.IsActive(startup) | Window.IsActive(fullscreenvideo) | Window.IsActive(fullscreengame) | Window.IsActive(visualisation) | Window.IsActive(DialogMusicInfo.xml) | Window.IsActive(DialogVideoInfo.xml)</param>
				</include>
				<control type="image">
					<texture colordiffuse="$VAR[PanelColorVar]" border="20">panels/scan-back.png</texture>
				</control>
				<control type="image">
					<top>27</top>
					<left>32</left>
					<width>80</width>
					<height>80</height>
					<animation effect="rotate" end="-360" center="auto" time="2000" loop="true" condition="true">Conditional</animation>
					<texture colordiffuse="$VAR[HighlightColorVar]">panels/spinner.png</texture>
				</control>
				<control type="grouplist">
					<top>10</top>
					<left>143</left>
					<width>550</width>
					<height>110</height>
					<align>center</align>
					<itemgap>4</itemgap>
					<control type="label">
						<description>Line 1 Label</description>
						<height>25</height>
						<label>$INFO[Control.GetLabel(30)]$INFO[Control.GetLabel(32), ,%]</label>
						<font>Font29_Cond</font>
					</control>
					<control type="label" id="31">
						<description>Line 2 Label</description>
						<height>30</height>
						<textcolor>grey</textcolor>
						<font>Font29_Cond</font>
					</control>
					<control type="progress" id="32">
					</control>
				</control>
			</control>
		</control>
		<control type="label" id="30">
			<description>Line 1 Label</description>
			<include>HiddenObject</include>
		</control>
	</controls>
</window>