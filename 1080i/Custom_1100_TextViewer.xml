<?xml version="1.0" encoding="utf-8"?>
<window id="1100" type="dialog">
	<defaultcontrol always="true">60</defaultcontrol>
	<include>Animation_Dialog_Popup_Window</include>
	<onunload>ClearProperty(TextViewerHeader,home)</onunload>
	<onunload>ClearProperty(TextViewerText,home)</onunload>
	<controls>
		<control type="group">
			<include content="Dialog_Background">
				<param name="width" value="1220" />
				<param name="height" value="790" />
				<param name="label">$INFO[Window(home).Property(TextViewerHeader)]</param>
			</include>
			<control type="textbox" id="100">
				<top>110</top>
				<left>60</left>
				<right>60</right>
				<bottom>90</bottom>
				<font>Font34_Reg</font>
				<pagecontrol>60</pagecontrol>
				<label>$INFO[Window(home).Property(TextViewerText)]</label>
			</control>
			<control type="label">
				<bottom>20</bottom>
				<right>70</right>
				<width>460</width>
				<height>50</height>
				<align>right</align>
				<font>Font25_Cond</font>
				<textcolor>grey</textcolor>
				<label>$INFO[Container(100).CurrentPage,$LOCALIZE[31000] ]$INFO[Container(100).NumPages,/]</label>
			</control>
			<!-- Scroll bar -->
			<control type="scrollbar" id="60">
				<left>1</left>
				<top>88</top>
				<height>420</height>
			</control>
		</control>
	</controls>
</window>