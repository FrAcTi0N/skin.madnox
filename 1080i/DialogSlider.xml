<?xml version="1.0" encoding="utf-8"?>
<window>
	<defaultcontrol>11</defaultcontrol>
	<include>AnimationDialogPopupWindow</include>
	<controls>
		<control type="group">
			<top>60</top>
			<include content="DialogBackground">
				<param name="width" value="700" />
				<param name="height" value="176" />
				<param name="id">10</param>
				<param name="diffusetexture">diffuse/rounded10-660x156.png</param>
			</include>
			<control type="label" id="12">
				<top>19</top>
				<right>60</right>
				<width>400</width>
				<height>70</height>
				<align>right</align>
				<font>Font33_Bold</font>
				<textcolor>colordiffuse="$VAR[DialogSubTextColorVar]"</textcolor>
			</control>
			<control type="slider" id="11">
				<top>105</top>
				<left>50</left>
				<width>600</width>
				<height>35</height>
				<texturesliderbar colordiffuse="$VAR[DialogTextColorVar]">buttons/slider-back2.png</texturesliderbar>
				<textureslidernib>buttons/slider-nofocus.png</textureslidernib>
				<textureslidernibfocus colordiffuse="$VAR[DialogButtonHighlightColorVar]">buttons/slider-focus.png</textureslidernibfocus>
				<orientation>horizontal</orientation>
				<onleft>11</onleft>
				<onright>11</onright>
			</control>
		</control>
	</controls>
</window>