<?xml version="1.0" encoding="utf-8"?>
<window>
	<defaultcontrol>20</defaultcontrol>
	<menucontrol>9050</menucontrol>
	<controls>
		<!-- Background -->
		<include content="Background_Default">
			<param name="imagepath">$VAR[SystemBackgroundVar]</param>
		</include>
		<!-- Floor -->
		<control type="group">
			<visible>!Skin.HasSetting(SettingsHideFloor)</visible>
			<include content="Animation_Slide_Bottom_Window">
				<param name="slide">220</param>
				<param name="opencondition">$EXP[System_Animation_OpenCondition]</param>
				<param name="closecondition">$EXP[System_Animation_CloseCondition]</param>
			</include>
			<include content="Animation_Slide_Bottom_Visible">
				<param name="slide">220</param>
			</include>
			<include>Floor_Default</include>
		</control>
		<!-- Panel -->
		<control type="group">
			<include>Animation_Slide_Left_Window</include>
			<include content="Panel_Default">
				<param name="right">965</param>
				<param name="righttexture">985</param>
				<param name="diffusetexture">diffuse/rounded20-835x880.png</param>
				<param name="reflect">!Skin.HasSetting(SettingsHideFloor)</param>
			</include>
		</control>
		<control type="group">
			<include>Animation_Slide_Right_Window</include>
			<include content="Panel_Default">
				<param name="left">965</param>
				<param name="lefttexture">985</param>
				<param name="diffusetexture">diffuse/rounded20-835x880.png</param>
				<param name="reflect">!Skin.HasSetting(SettingsHideFloor)</param>
			</include>
		</control>
		<!-- Content -->
		<control type="group">
			<!-- Left Panel -->
			<control type="group">
				<include>Animation_Slide_Left_Window</include>
				<include content="FileManager_List">
					<param name="id">20</param>
					<param name="left">100</param>
					<param name="onleft">9050</param>
					<param name="onright">60</param>
					<param name="scrollbar_id">60</param>
				</include>
				<control type="scrollbar" id="60">
					<left>915</left>
					<top>144</top>
					<bottom>144</bottom>
					<onleft>20</onleft>
					<onright>21</onright>
				</control>
				<control type="label" id="101">
					<bottom>102</bottom>
					<left>145</left>
					<width>615</width>
					<height>40</height>
					<font>Font22_Reg</font>
					<textcolor>$VAR[PanelTextColorVar]</textcolor>
					<haspath>true</haspath>
				</control>
			</control>
			<!-- Right Panel -->
			<control type="group">
				<include>Animation_Slide_Right_Window</include>
				<include content="FileManager_List">
					<param name="id">21</param>
					<param name="left">985</param>
					<param name="onleft">60</param>
					<param name="onright">61</param>
					<param name="scrollbar_id">61</param>
				</include>
				<control type="scrollbar" id="61">
					<right>80</right>
					<top>144</top>
					<bottom>144</bottom>
					<onleft>21</onleft>
				</control>
				<control type="label" id="102">
					<bottom>102</bottom>
					<left>1030</left>
					<width>615</width>
					<height>40</height>
					<font>Font22_Reg</font>
					<textcolor>$VAR[PanelTextColorVar]</textcolor>
					<haspath>true</haspath>
				</control>
			</control>
		</control>
		<!-- Topbar -->
		<control type="group">
			<visible>!Skin.HasSetting(SettingsHideTopBar)</visible>
			<include content="Animation_Slide_Top_Window">
				<param name="slide">90</param>
				<param name="opencondition">$EXP[System_Animation_OpenCondition]</param>
				<param name="closecondition">$EXP[System_Animation_CloseCondition]</param>
			</include>
			<include content="Animation_Slide_Top_Visible">
				<param name="slide">90</param>
			</include>
			<include content="TopBar">
				<param name="breadcrumbs_label">$LOCALIZE[13000] / $LOCALIZE[7]</param>
				<param name="breadcrumbs_icon">files.png</param>
			</include>
		</control>
		<!-- Media Menu -->
		<include>MediaMenu_MouseOverlay</include>
		<control type="group">
			<include>MediaMenu_Common</include>
			<control type="grouplist" id="9050">
				<onright>20</onright>
				<onback>20</onback>
				<include>MediaMenu_System</include>
				<include>MediaMenu_Common_Bottom</include>
			</control>
		</control>
	</controls>
</window>