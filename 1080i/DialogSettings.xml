<?xml version="1.0" encoding="utf-8"?>
<window>
	<defaultcontrol always="true">5</defaultcontrol>
	<include>AnimationDialogPopupWindow</include>
	<controls>
		<control type="group">
			<visible>!Window.IsVisible(DialogSlider.xml) + !Window.IsVisible(filebrowser)</visible>
			<include>AnimationDialogPopupVisible</include>
			<include content="DialogBackground">
				<param name="width" value="1220" />
				<param name="height" value="790" />
				<param name="id" value="2" />
			</include>
			<control type="grouplist" id="5">
				<left>20</left>
				<top>90</top>
				<width>858</width>
				<height>630</height>
				<onleft>60</onleft>
				<onright>8000</onright>
				<pagecontrol>60</pagecontrol>
			</control>
			<control type="button" id="7">
