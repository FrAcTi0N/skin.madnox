				<left>20</left>
				<right>20</right>
				<height>70</height>
				<texture border="30" colordiffuse="$VAR[DialogListSeparatorColorVar]">buttons/list-select-nofocus.png</texture>
			</control>
			<control type="image">
				<visible>Integer.IsGreater(Container(5).NumItems,8)</visible>
				<bottom>70</bottom>
				<left>858</left>
				<right>20</right>
				<height>70</height>
				<texture border="30" colordiffuse="$VAR[DialogListSeparatorColorVar]">buttons/list-select-nofocus.png</texture>
			</control>
			<control type="grouplist" id="8000">
