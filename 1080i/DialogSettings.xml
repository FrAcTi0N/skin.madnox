					<param name="id">28</param>
				</include>
				<include content="DialogButtonAction">
					<param name="id">29</param>
				</include>
				<include content="DialogButtonAction">
					<param name="id">30</param>
				</include>
			</control>
			<control type="scrollbar" id="60">
				<onright>5</onright>
				<include>DialogScrollBar</include>
			</control>
			<control type="label">
				<visible>ControlGroup(5).HasFocus</visible>
				<bottom>21</bottom>
				<right>60</right>
				<width>460</width>
				<height>50</height>
				<align>right</align>
				<font>Font25_Cond</font>
				<textcolor>$VAR[DialogSubTextColorVar]</textcolor>
				<label>$INFO[Container(5).CurrentItem,$LOCALIZE[31001] ]$INFO[Container(5).NumItems,/]</label>
			</control>
			<control type="label">
				<visible>!ControlGroup(5).HasFocus</visible>
				<bottom>21</bottom>
				<right>60</right>
				<width>460</width>
				<height>50</height>
				<align>right</align>
				<font>Font25_Cond</font>
				<textcolor>$VAR[DialogSubTextColorVar]</textcolor>
				<label>$INFO[Container(5).NumItems,$LOCALIZE[31001] 1/]</label>
			</control>
