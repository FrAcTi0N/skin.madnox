			</control>
			<control type="spincontrolex" id="9">
				<include>DialogButton</include>
			</control>
			<control type="edit" id="12">
				<include>DialogButton</include>
			</control>
			<control type="sliderex" id="13">
				<include>DialogButton</include>
			</control>
			<control type="label" id="14">
				<include>DialogLabel</include>
			</control>
			<control type="image">
				<top>90</top>
				<left>878</left>
				<bottom>71</bottom>
				<width>4</width>
				<texture colordiffuse="$VAR[DialogListSeparatorColorVar]">panels/line-vertical.png</texture>
			</control>
			<control type="image">
				<visible>!Integer.IsGreater(Container(5).NumItems,8)</visible>
