<?xml version="1.0" encoding="utf-8"?>
<includes>
	<include name="Animation_VisibleFade">
		<animation effect="fade" end="100" time="200" reversible="false">Visible</animation>
		<animation effect="fade" start="100" end="0" time="200" reversible="false">Hidden</animation>
	</include>
	<include name="Animation_WindowFade">
		<animation effect="fade" end="100" time="200" reversible="false">WindowOpen</animation>
		<animation effect="fade" start="100" end="0" time="200" reversible="false">WindowClose</animation>
	</include>
	<include name="Animation_Dialog_Popup_Window">
		<animation type="WindowOpen" reversible="false">
			<effect type="zoom" start="80" end="100" center="50%,50%" delay="160" tween="back" time="240" />
			<effect type="fade" delay="160" end="100" time="240" />
		</animation>
		<animation type="WindowClose" reversible="false">
			<effect type="zoom" start="100" end="80" center="50%,50%" easing="in" tween="back" time="240" />
			<effect type="fade" start="100" end="0" time="240" />
		</animation>
	</include>
	<include name="Animation_Dialog_Popup_Visible">
		<animation type="Visible" reversible="false">
			<effect type="zoom" start="80" end="100" center="auto" delay="160" tween="back" time="240" />
			<effect type="fade" delay="160" end="100" time="240" />
		</animation>
		<animation type="Hidden" reversible="false">
			<effect type="zoom" start="100" end="80" center="auto" easing="in" tween="back" time="240" />
			<effect type="fade" start="100" end="0" time="240" />
		</animation>
	</include>
	<include name="Animation_Dialog_MediaMenu_Window">
		<param name="slide">430</param>
		<definition>
			<animation type="WindowOpen" reversible="false">
				<effect type="fade" start="0" end="100" time="300" tween="sine" easing="out" />
				<effect type="slide" start="0" end="$PARAM[slide]" time="400" tween="cubic" easing="out" />
			</animation>
			<animation type="WindowClose" reversible="false">
				<effect type="fade" start="100" end="0" time="300" tween="sine" easing="out" />
				<effect type="slide" start="$PARAM[slide]" end="0" time="300" tween="cubic" easing="out" />
			</animation>
		</definition>
	</include>
	<include name="Animation_Dialog_MediaMenu_Visible">
		<param name="slide">430</param>
		<definition>
			<animation type="Visible" reversible="false">
				<effect type="fade" start="0" end="100" time="300" tween="sine" easing="out" />
				<effect type="slide" start="-$PARAM[slide]" end="0" time="400" tween="cubic" easing="out" />
			</animation>
			<animation type="Hidden" reversible="false">
				<effect type="fade" start="100" end="0" time="300" tween="sine" easing="out" />
				<effect type="slide" start="0" end="-$PARAM[slide]" time="300" tween="cubic" easing="out" />
			</animation>
		</definition>
	</include>
	<include name="Animation_Zoom_Window">
		<animation type="WindowOpen" reversible="false">
			<effect type="zoom" start="120" end="100" center="50%,50%" time="300" tween="cubic" easing="out" />
			<effect type="fade" start="0" end="100" time="300"/>
		</animation>
		<animation type="WindowClose" reversible="false">
			<effect type="fade" start="100" end="0" time="300"/>
		</animation>
	</include>0
	<include name="Animation_Slide_Top_Window">
		<param name="slide">1080</param>
		<param name="opencondition">true</param>
		<param name="closecondition">true</param>
		<definition>
			<animation type="WindowOpen" reversible="false" condition="$PARAM[opencondition]">
				<effect type="fade" start="0" end="100" time="300"/>
				<effect type="slide" start="0,-$PARAM[slide]" end="0,0" time="300" tween="cubic" easing="out" />
			</animation>
			<animation type="WindowClose" reversible="false" condition="$PARAM[closecondition]">
				<effect type="fade" start="100" end="0" time="300"/>
				<effect type="slide" start="0,0" end="0,-$PARAM[slide]" time="300" tween="cubic" easing="out" />
			</animation>
		</definition>
	</include>
