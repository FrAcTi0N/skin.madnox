			<!-- Overlay -->
			<control type="image">
				<include>FullscreenDimensions</include>
				<texture colordiffuse="DDFFFFFF">colors/black.png</texture>
				<visible>$PARAM[overlayvisible] + String.IsEmpty(Skin.String(BackgroundOverlay)) + String.IsEmpty(Skin.String(GlobalBackground.Color.base)) + String.IsEmpty(Skin.String($PARAM[backgroundcolor]))</visible>
			</control>
			<control type="image">
				<include>FullscreenDimensions</include>
				<texture colordiffuse="$VAR[BackgroundColorVar]">colors/white.png</texture>
				<visible>$PARAM[overlayvisible] + String.IsEmpty(Skin.String(BackgroundOverlay)) + [!String.IsEmpty(Skin.String(GlobalBackground.Color.base)) | !String.IsEmpty(Skin.String($PARAM[backgroundcolor]))]</visible>
			</control>
			<control type="image">
				<include>FullscreenDimensions</include>
				<texture colordiffuse="$VAR[BackgroundColorVar]">$INFO[Skin.String(BackgroundOverlay)]</texture>
				<visible>!String.IsEmpty(Skin.String(BackgroundOverlay))</visible>
			</control>
		</definition>
	</include>

	<include name="HomeBackground">
		<control type="multiimage">
			<include>FullscreenDimensions</include>
			<aspectratio>scale</aspectratio>
			<imagepath background="true">$PARAM[imagepath]</imagepath>
			<randomize>true</randomize>
			<fadetime>TextureFadeTime</fadetime>
			<animation type="Conditional" condition="Skin.HasSetting(AnimatedBackground)">
				<effect type="zoom" start="110" end="130" center="auto" time="12000"/>
				<effect type="slide" start="-15,-15" end="15,15" time="6000"/>
			</animation>
			<visible>String.IsEmpty(Container(9000).ListItem.Property(backgroundPlaylist))</visible>
		</control>
		<include>skinshortcuts-template-BackgroundPlaylistHome</include>
	</include>
