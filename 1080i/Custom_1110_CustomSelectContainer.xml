<?xml version="1.0" encoding="utf-8"?>
<window id="1110" type="dialog">
	<defaultcontrol always="true">100</defaultcontrol>
	<include>Animation_Dialog_Popup_Window</include>
	<onunload>ClearProperty(CustomSelect,home)</onunload>
	<controls>
		<control type="group">
			<visible>!Window.IsActive(DialogSelect.xml) + !Window.IsActive(script-skin_helper_service-ColorPicker.xml)</visible>
			<include>Animation_Dialog_Popup_Visible</include>
			<include content="Dialog_Background">
				<param name="width" value="1030" />
				<param name="height" value="580" />
				<param name="label">$INFO[Window(home).Property(CustomSelect)]</param>
			</include>
			<control type="list" id="100">
				<left>20</left>
				<top>90</top>
				<width>990</width>
				<height>420</height>
				<pagecontrol>60</pagecontrol>
				<onleft>60</onleft>
				<onright>8000</onright>
				<onup>8000</onup>
				<include content="Dialog_List2_Default">
					<param name="id">100</param>
				</include>
				<content>
					<!-- Colors -->
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[31106] $LOCALIZE[31105])">SelectGlobalColors</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[10000] $LOCALIZE[31105])">SelectHomeColors</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[3] $LOCALIZE[31105])">SelectVideoColors</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[3] $LOCALIZE[31105])">SelectMovieColors</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[3] $LOCALIZE[31105])">SelectTVShowColors</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[3] $LOCALIZE[31105])">SelectMusicVideoColors</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[14204] $LOCALIZE[31105])">SelectTVColors</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[14204] $LOCALIZE[31105])">SelectRadioColors</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[2] $LOCALIZE[31105])">SelectMusicColors</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[15016] $LOCALIZE[31105])">SelectGameColors</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[1] $LOCALIZE[31105])">SelectPictureColors</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[24001] $LOCALIZE[31105])">SelectAddonColors</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[8] $LOCALIZE[31105])">SelectWeatherColors</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[13000] $LOCALIZE[31105])">SelectSystemColors</include>
					<!-- Background effect -->
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),Effects)">SelectBackgroundEffect</include>
					<!-- Background overlay -->
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),Overlay)">SelectBackgroundOverlay</include>
					<!-- Backgrounds -->
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[3] $LOCALIZE[31107])">SelectVideoBackground</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[3] $LOCALIZE[31107])">SelectMoviesBackground</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[3] $LOCALIZE[31107])">SelectTVShowsBackground</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[3] $LOCALIZE[31107])">SelectMusicVideoBackground</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[14204] $LOCALIZE[31107])">SelectTVBackground</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[14204] $LOCALIZE[31107])">SelectRadioBackground</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[2] $LOCALIZE[31107])">SelectMusicBackground</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[15016] $LOCALIZE[31107])">SelectGamesBackground</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[1] $LOCALIZE[31107])">SelectPicturesBackground</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[24001] $LOCALIZE[31107])">SelectAddonBackground</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[8] $LOCALIZE[31107])">SelectWeatherBackground</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[13000] $LOCALIZE[31107])">SelectSystemBackground</include>
					<!-- Main menu -->
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[33061] $LOCALIZE[31051])">SelectMainMenuTexture</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[33061] $LOCALIZE[736])">SelectMainMenuStyle</include>
					<!-- Widget -->
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),Opacity)">SelectWidgetOpacity</include>
				
					<!-- Style -->
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[31056] $LOCALIZE[31129])">SelectPanelStyle</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[31128] $LOCALIZE[31129])">SelectDialogStyle</include>

					<!-- Texture -->
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[31056] $LOCALIZE[31132])">SelectPanelTexture</include>
					<include condition="String.IsEqual(Window(home).Property(CustomSelect),$LOCALIZE[31128] $LOCALIZE[31132])">SelectDialogTexture</include>
				</content>
			</control>
			<control type="label">
				<bottom>20</bottom>
				<right>70</right>
				<width>460</width>
				<height>50</height>
				<align>right</align>
				<font>Font25_Cond</font>
				<textcolor>$VAR[DialogSubTextColorVar]</textcolor>
				<label>$INFO[Container(100).NumItems,, $LOCALIZE[31001]]$INFO[Container(100).CurrentPage, - ]$INFO[Container(100).NumPages,/]</label>
			</control>
			<!-- Scroll bar -->
			<control type="scrollbar" id="60">
				<left>1</left>
				<top>88</top>
				<height>420</height>
				<onright>100</onright>
			</control>
		</control>
	</controls>
</window>